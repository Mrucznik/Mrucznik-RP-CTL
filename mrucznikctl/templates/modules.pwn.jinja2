//-------<[ .def ]>-------
{% for module in modules %}#include "{{module.name}}\{{module.name}}.def"
{% endfor %}

//-------<[ .hwn ]>-------
{% for module in modules %}#include "{{module.name}}\{{module.name}}.hwn"
{% endfor %}

//-------<[ .pwn ]>-------
{% for module in modules %}#include "{{module.name}}\{{module.name}}.pwn"
{% endfor %}

//-------<[ timers ]>-------
{% for module in modules %}{% if module.timers -%}#include "{{module.name}}\{{module.name}}_timers.pwn"
{% endif %}{% endfor %}

//-------<[ mysql ]>-------
{% for module in modules %}{% if module.mysql -%}#include "{{module.name}}\{{module.name}}_mysql.pwn"
{% endif %}{% endfor %}

//-------<[ others ]>-------
